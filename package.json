{
  "name": "urban-pulse-backend",
  "version": "1.0.0",
  "description": "Urban Pulse ride-sharing backend monorepo",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.18.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeRisshi25/conductor.git"
  },
  "keywords": [
    "monorepo",
    "ride-sharing",
    "express",
    "typescript",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codeRisshi25/conductor/issues"
  },
  "homepage": "https://github.com/codeRisshi25/conductor#readme"
}
